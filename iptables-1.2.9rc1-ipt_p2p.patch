diff -Naur iptables-1.2.9rc1.orig/extensions/libipt_p2p.c iptables-1.2.9rc1/extensions/libipt_p2p.c
--- iptables-1.2.9rc1.orig/extensions/libipt_p2p.c	1970-01-01 01:00:00.000000000 +0100
+++ iptables-1.2.9rc1/extensions/libipt_p2p.c	2003-11-04 22:39:29.000000000 +0100
@@ -0,0 +1,63 @@
+/* Shared library add-on to iptables for unclean. */
+#include <stdio.h>
+#include <stdlib.h>
+#include <getopt.h>
+#include <iptables.h>
+
+/* Function which prints out usage message. */
+static void
+help(void)
+{
+	printf(
+"p2p v%s takes no options\n"
+"\n", IPTABLES_VERSION);
+}
+
+static struct option opts[] = {
+	{0}
+};
+
+/* Initialize the match. */
+static void
+init(struct ipt_entry_match *m, unsigned int *nfcache)
+{
+	/* Can't cache this. */
+	*nfcache |= NFC_UNKNOWN;
+}
+
+/* Function which parses command options; returns true if it
+   ate an option */
+static int
+parse(int c, char **argv, int invert, unsigned int *flags,
+      const struct ipt_entry *entry,
+      unsigned int *nfcache,
+      struct ipt_entry_match **match)
+{
+	return 0;
+}
+
+/* Final check; must have specified --mac. */
+static void final_check(unsigned int flags)
+{
+}
+
+static
+struct iptables_match p2p
+= { NULL,
+    "p2p",
+    IPTABLES_VERSION,
+    IPT_ALIGN(0),
+    IPT_ALIGN(0),
+    &help,
+    &init,
+    &parse,
+    &final_check,
+    NULL, /* print */
+    NULL, /* save */
+    opts
+};
+
+void _init(void)
+{
+	register_match(&p2p);
+}
